<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mhit.davis.contactloader">

    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:name="com.mhit.davis.contactloader.ContactLoaderApplication"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="com.mhit.davis.contactloader.activity.MainActivity"
            android:configChanges="orientation|keyboard|screenSize">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.mhit.davis.contactloader.activity.ContactActivity"
            android:configChanges="orientation|keyboard|screenSize"/>
    </application>

</manifest>